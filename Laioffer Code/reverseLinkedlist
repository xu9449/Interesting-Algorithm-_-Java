public class reverseLinkedlist {
  public ListNode reverse(ListNode head) {
    // Write your solution here
    ListNode prev = null, cur = head;
    while(cur!= null){
      ListNode next = cur.next;  
      cur.next = prev;  
      prev = cur;
      cur = next;
      
  }
    return prev;
}
}
//如果将 next = head.next写在外面 
//需要多写两个check 1） head ！= null 2） next！=null
//Time O(n)  Space O(1) 
