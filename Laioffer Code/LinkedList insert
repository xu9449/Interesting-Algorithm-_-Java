public class reverseLinkedlist {
  public ListNode insert(ListNode head, int target) {  
  if ï¼ˆhead == null || target <= head.value) {
    ListNode newHead = new ListNode(target);
    newHead.next = head;
    return newHead;
  } 
  ListNode prev = null;
  ListNode curr = head;
  while (curr != null) {
  if (curr.value < target) {
        prev = curr;
        curr = curr.next;
        } else {
        prev.next = new ListNode(target);  
        prev.next.next = curr;
        return head;          
        }
  }
  prev.next = new ListNode(target);
  return head;
  }

 